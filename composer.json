{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "libs/evergreen-generic",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "evergreen/generic": "dev-main",
    "askedio/laravel-soft-cascade": "5.5.*",
    "barryvdh/laravel-debugbar": "3.1.*",
    "barryvdh/laravel-snappy": "^0.4.4",
    "fideloper/proxy": "~3.3",
    "h4cc/wkhtmltopdf-i386": "^0.12.4",
    "iio/libmergepdf": "^4.0",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "~1.0"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Evergreen\\Generic\\": "libs/evergreen-generic"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "symfony/thanks": true
    }
  }
}